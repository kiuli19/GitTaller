<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Taller2012\BaseBundle\Entity\Usuario" table="usuario">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="idusuario" type="integer" column="idUsuario">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="45"/>
    <field name="password" type="string" column="password" length="45"/>
    <field name="email" type="string" column="email" length="45"/>
    <field name="preguntasecreta" type="string" column="preguntaSecreta" length="60"/>
    <field name="respuestapregunta" type="string" column="respuestaPregunta" length="60"/>
    <field name="activo" type="boolean" column="activo"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
